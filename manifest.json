{
  "name": "Flipband",
  "version": "1.0",
  "description": "The easiest way to remember your goals",
  "manifest_version" : 2,
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides" : {
    "newtab": "flipband.html"
  },
  "permissions": [
    "storage"
  ]
}